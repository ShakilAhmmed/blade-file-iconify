{
    "name": "shakilahmmed/blade-file-iconify",
    "description": "A package to easily make use of Iconify in your Laravel Blade views.",
    "keywords": [
        "Blade",
        "Icons",
        "Laravel",
        "SVG",
        "Iconify"
    ],
    "homepage": "https://github.com/ShakilAhmmed/blade-file-iconify",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ShakilAhmmed/blade-file-iconify/issues",
        "source": "https://github.com/ShakilAhmmed/blade-file-iconify"
    },
    "authors": [
        {
            "name": "Shakil Ahmmed",
            "homepage": "https://github.com/ShakilAhmmed"
        }
    ],
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ShakilAhmmed"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/view": "^8.0|^9.0|^10.0|^11.0",
        "symfony/console": "^5.3|^6.0|^7.0",
        "symfony/finder": "^5.3|^6.0|^7.0",
        "brick/varexporter": "^0.1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5|^11.0"
    },
    "bin": [
        "bin/blade-icons-generate"
    ],
    "autoload": {
        "psr-4": {
            "ShakilAhmmed\\BladeFileIconify\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShakilAhmmed\\BladeFileIconify\\BladeFileIconifyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
